{
  "env": {
    "browser": true
  },
  "rules": {
    "consistent-return": 2,
    "complexity": [
      2,
      3
    ],
    "curly": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "no-alert": 2,
    "no-caller": 2,
    "no-console": 2,
    "no-else-return": 2,
    "no-multi-spaces": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "no-invalid-this": 2,
    "vars-on-top": 2,
    "yoda": 2
  }
}
